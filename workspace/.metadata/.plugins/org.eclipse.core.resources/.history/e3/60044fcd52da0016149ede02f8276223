package com.lizhengxian.basic;

public class Demo {
      private int item;
      public static void main(String[]args){
    	  Demo d = new Demo();
    	  Demo c = new Demo();
    	  int[] a = new int[]{1,2,3,4};
    	  d.item = a[0];
    	  c.item = a[3];
    	  d=c;
    	  System.out.println(d.item);
    	  //System.out.println(c);
    	  
      }
}
