package com.lizhengxian.linked;

public class Demo {
      public static void main(String args[]){
    	  LinkedDemo<String> ld =new LinkedDemo<String>();
    	  ld.addFirst("a");
    	  ld.addLast("dddd");
    	  System.out.println(ld.first.item);
    	  System.out.println("........");
    	  int[] iArr=new int[]{1,2,3};
    	  int[] bArr=new int[]{4,5,6};
    	  final int[] i=iArr;
    	  iArr=bArr;
    	  System.out.println(i[0]);
    	  
      }
     
}
