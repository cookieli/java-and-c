package com.lizhengxian.basic;

public class IterativeTrieST<Value> {
	 private static int R = 256 ;
	 private Node root;
     private static class Node{
    	 private Object val  ;
    	 private Node[] next = new Node[R];
     }
    public void put(String key,Value val){
    	if(root == null) root = new Node();
    	Node x = root ;
    	Node pre = null ;
    	int d =0 ;
    	while(d<key.length()){
    		pre = x ;
    		char c = key.charAt(d);
    		x = x.next[c];
    		if(x==null)  pre.next[c] = new Node();
    		x = pre.next[c];
    		d++;
    		if(d == key.length()) {
    			x.val = val;return;
    		}
    	}
    }
    public  void insert(String key,Value val){
    	   insert(root,key,val);
    }
    private void insert(Node proot,String key,Value val){
    	if(proot == null) proot = new Node();
    	for(int i = 0 ; i < key.length() ; i++){
    		char c  = key.charAt(i);
    		if(i == key.length()-1) {
    			proot.val = val;
    		}
    		if(proot.next[c]==null){
    			proot.next[c] = new Node();
    		}
    		proot = proot.next[c];
    	}
    }
     
     public Value get(String key){
    	 Node x = root ;
    	 int d = 0 ;
    	 if(x == null) return  null;
    	 while(d<key.length()&&x!=null){
    		 char c = key.charAt(d);
    		 x = x.next[c];
    		 d++;
    	 }
    	 return (Value)x.val;
     }
     public static void main(String[]args){
    	 IterativeTrieST<Integer> st = new IterativeTrieST<Integer>();
           st.insert("a", 1);
    	   System.out.println(st.root.next['a']);
    	
    	 
     }
}
