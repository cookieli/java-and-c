package com.lizhengxian.linked;

public class Demo {
      public static void main(String args[]){
    	  LinkedDemo<String> ld =new LinkedDemo<String>();
    	  ld.addFirst("a");
    	  ld.addLast("dddd");
    	  ld.addLast("ccc");
    	  ld.addLast("eee");
    	  ld.addLast("gggg");
    	  ld.insertAfter(ld.find(0), ld.find(3));
         // ld.removeAfter(ld.find(2));
          System.out.println(ld.size());
          System.out.println(ld.find(0).item);
          System.out.println(ld.find(1).item);
          System.out.println(ld.find(2).item);
          System.out.println(ld.find(3).item);
          System.out.println(ld.find(4).item);
          System.out.println(ld.find(5).item);
          System.out.println(ld.find(6).item);
          //System.out.println(ld.find(7));
          
      }
     
}
